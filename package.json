{
  "name": "@umami/esbuild-plugin-css-modules",
  "version": "0.4.0",
  "description": "A esbuild plugin for bundling CSS modules",
  "author": "Umami <hello@umami.is>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/umami-software/esbuild-plugin-css-modules"
  },
  "keywords": ["esbuild", "plugin", "css", "css-modules"],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "module": "index.mjs",
  "files": [
    "index.js",
    "index.mjs"
  ],
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "scripts": {
    "build": "esbuild index.mjs --format=cjs --outfile=index.js"
  },
  "dependencies": {
    "postcss": "^8.4.49",
    "postcss-modules": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "esbuild": "^0.24.0"
  }
}
